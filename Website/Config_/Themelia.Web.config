<themelia.web>
  <webDomains>
    <add defaultPage="/Page_/Blog/Root.aspx" catchAllMode="DefaultPage">
      <components>
        <add key="Minima" type="Minima.Web.Processing.MinimaComponent, Minima.Web">
          <parameters>
            <add name="blogGuid" value="19277C41-7E4D-4AE0-A196-25F45AC48762" />
          </parameters>
        </add>
      </components>
      <endpoints>
        <add matchType="webDomainPathEquals" type="ServiceAlias" text="/service/([a-z0-9]+)/([a-z0-9]+)" defaultParameter="/Service_/$1.svc/web/$2" />
      </endpoints>
      <accessDenials>
        <add name="local" type="address" text="127.0.0.1" message="Your IP address has been blocked" target="" enabled="false" />
      </accessDenials>
    </add>
    <add name="second" path="/second/" defaultPage="/Page_/Blog/Second.aspx" basedOn="root">
      <components>
        <add key="Minima">
          <parameters>
            <add name="blogGuid" value="B23115B1-42E8-46A2-88DE-A56CE505E7D0" />
          </parameters>
        </add>
      </components>
    </add>
    <add name="proxy" path="/proxy/" defaultPage="~/Page_/Information/Proxy.aspx">
      <components>
        <add key="Minima" type="Minima.Web.Processing.MinimaProxyComponent, Minima.Web" />
      </components>
    </add>
  </webDomains>
</themelia.web>